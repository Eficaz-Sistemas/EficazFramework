<ResourceDictionary
    xmlns:themes="clr-namespace:EficazFramework.Themes"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>

        <!-- Color Scheme: -->
        <ResourceDictionary Source="Colors/BaseColors.xaml"/>
        <themes:ColorDictionary Source="Colors/DefaultBlue.xaml"/>

        <!-- Icons: -->
        <ResourceDictionary Source="pack://application:,,,/EficazFramework.WPF;component/Themes/Icons/App.xaml" />
        <ResourceDictionary Source="pack://application:,,,/EficazFramework.WPF;component/Themes/Icons/Brands.xaml" />
        <ResourceDictionary Source="pack://application:,,,/EficazFramework.WPF;component/Themes/Icons/Commerce.xaml" />
        <ResourceDictionary Source="pack://application:,,,/EficazFramework.WPF;component/Themes/Icons/MaterialDesign.xaml" />
        

    </ResourceDictionary.MergedDictionaries>

    <!-- Fonts: -->
    <FontFamily x:Key="Font.SourceSansPro">/Themes/Fonts/SourceSansPro-Regular.ttf#Source Sans Pro</FontFamily>

    

</ResourceDictionary>