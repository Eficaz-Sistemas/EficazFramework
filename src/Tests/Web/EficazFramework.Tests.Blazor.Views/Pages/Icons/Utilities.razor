@page "/Icons/Utilities"
@inject EficazFramework.Services.IThemeProvider ThemeProvider

<div class="d-flex flex-grow-1">
    <div style="width: 15%; background-image: url('@(EficazFramework.Icons.Utilities.ToUri(EficazFramework.Icons.Brands.EficazGraphism, viewbox: "0 0 70 24"))');"></div>
    <div style="width: 15%; background-image: url('@(EficazFramework.Icons.Utilities.ToUri(EficazFramework.Icons.Brands.EficazGraphism, "red", "0 0 70 24"))');"></div>
    <div style="width: 15%; background-image: url('@(EficazFramework.Icons.Utilities.ToUri(EficazFramework.Icons.Brands.EficazGraphism, ThemeProvider.Theme.PaletteLight.Primary.Value, "0 0 70 24"))');"></div>
    <div style="width: 15%; background-image: url('@(EficazFramework.Icons.Utilities.ToUri(EficazFramework.Icons.Brands.EficazGraphism, ThemeProvider.Theme.PaletteLight.Secondary.Value, "0 0 70 24"))');"></div>
    <div style="width: 15%; background-image: url('@(EficazFramework.Icons.Utilities.ToUri(EficazFramework.Icons.Brands.EficazGraphism, ThemeProvider.Theme.PaletteLight.Tertiary.Value, "0 0 70 24"))');"></div>
</div>